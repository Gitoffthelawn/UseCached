{

  "description": "switch webpage to google webcache.",
  "manifest_version": 2,
  "name": "UseCached",
  "version": "1.0",
  "homepage_url": "https://github.com/fatzombi/UseCached",

  "applications": {
    "gecko": {
      "id": "usecached@mozilla.org",
      "strict_min_version": "49.0"
    }
  },

  "background": {
    "scripts": ["background.js"]
  },

  "page_action": {
    "default_icon": "icons/off.svg",
    "browser_style": true
  },

  "permissions": [
    "http://*/*",
    "https://*/*",
    "activeTab",
    "tabs"
  ]

}
